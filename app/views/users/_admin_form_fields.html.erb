<div class="form-group">
  <%= f.label :name, class: "col-sm-3 col-xs-12 control-label" %>
  <div class="col-sm-9 col-xs-12">
    <%= f.text_field :name, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :username, class: "col-sm-3 col-xs-12 control-label" %>
  <div class="col-sm-9 col-xs-12">
    <%= f.text_field :username, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :password, class: "col-sm-3 col-xs-12 control-label" %>
  <div class="col-sm-9 col-xs-12">
    <%= f.password_field :password, class: "form-control" %>
  </div>
</div>
<div class="form-group">
  <%= f.label :password_confirmation, "Confirm", class: "col-sm-3 col-xs-12 control-label" %>
  <div class="col-sm-9 col-xs-12">
    <%= f.password_field :password_confirmation, class: "form-control" %>
  </div>
</div>
<div class="row align-left">
  <div class="form-group col-sm-9 col-sm-push-3 col-xs-12">
    <label for="user_rehearsal" class="checkbox-inline inline-checkbox-fix margin-left-10">
      <%= f.check_box :rehearsal %>
      Rehearsal
    </label>
    <label for="user_rehearsal_dinner" class="checkbox-inline inline-checkbox-fix">
      <%= f.check_box :rehearsal_dinner %>
      Rehearsal Dinner
    </label>
    <label for="user_chicago_shower" class="checkbox-inline inline-checkbox-fix">
      <%= f.check_box :chicago_shower %>
      Chicago Shower
    </label>
    <label for="user_tipton_shower" class="checkbox-inline inline-checkbox-fix">
      <%= f.check_box :tipton_shower %>
      Tipton Shower
    </label>
  </div>
</div>
